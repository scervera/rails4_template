<!DOCTYPE html>
<html lang="en">
  <%= render "/shared/header" %>

  <body id="main">
    <div id="wrapper">
        <!-- BEGIN Website content -->
        <%= render "/shared/masthead" %>
        <%= render "/shared/navbar" %>
        <div id="main_content">
          <div class="large-8 column"><%= yield %></div>
          <div class="large-4 column"><%= render "/shared/side_bar"%></div>
        </div>
        <%= render "/shared/footer" %>
        <%= javascript_include_tag "application" %>
        <!-- END Website content -->
    </div>
  </body>
</html>